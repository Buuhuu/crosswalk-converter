{
  "name": "crosswalk-converter",
  "version": "0.0.1",
  "description": "A serverless wrapper of @adobe/helix-importer to facilitate projects to migrate from AEM to AEM with Edge Delivery Services.",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint .",
    "postinstall": "node bin/postinstall.mjs",
    "build": "rimraf dist/ && webpack",
    "build:prod": "rimraf dist/ && webpack --mode=production",
    "test": "instant-mocha",
    "e2e:build": "rimraf dist/ && webpack ./e2e/runtime.js",
    "e2e:build:prod": "rimraf dist/ && webpack ./e2e/runtime.js --mode=production",
    "e2e:serve": "rimraf dist/ && webpack ./e2e/dev-server.js && node --inspect ./dist/index.js --watch ./dist"
  },
  "mocha": {
    "spec": "test/**/*.test.js",
    "require": "test/setup-env.esm.mjs",
    "timeout": 10000
  },
  "author": "Dirk Rudolph <drudolph@adobe.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/github": "^6.0.0",
    "@adobe/helix-html-pipeline": "^4.1.10",
    "@adobe/helix-importer": "^3.0.0",
    "@adobe/mdast-util-gridtables": "^2.0.2",
    "@adobe/remark-gridtables": "^1.0.4",
    "dotenv": "^16.3.1",
    "eslint-plugin-mocha": "^10.2.0",
    "express": "4.18.2",
    "hast-util-from-html": "^2.0.1",
    "hast-util-raw": "^8.0.0",
    "hast-util-select": "^5.0.5",
    "hast-util-to-html": "^8.0.4",
    "hast-util-to-xast": "^3.0.0",
    "hastscript": "^7.2.0",
    "html-entities": "^2.4.0",
    "instant-mocha": "^1.5.2",
    "jsdom": "^22.1.0",
    "mdast-util-to-hast": "^12.3.0",
    "nock": "^13.3.7",
    "node-fetch": "3.3.1",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "openwhisk": "3.21.7",
    "raw-loader": "^4.0.2",
    "rehype-format": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "remark-stringify": "^11.0.0",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.9",
    "unified": "^11.0.4",
    "unist-util-position": "^5.0.0",
    "unist-util-visit": "^5.0.0",
    "unist-util-visit-parents": "^6.0.1",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "xast-util-to-xml": "^4.0.0",
    "yaml-loader": "^0.8.0",
    "zip-webpack-plugin": "^4.0.1",
    "zwitch": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.27.5"
  }
}
